<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="UTF-8">
    <title>Usuários</title>
</head>
<body>
    <h1>Lista de Usuários</h1>
    <ul>
        <% usuarios.forEach(usuario => { %>
            <li><%= usuario.nome_usuario %> | <%= usuario.email_usuario %></li>
        <% }) %>
    </ul>
</body>
</html>
